{
    "expo": {
        "name": "gedankenfehler",
        "slug": "gedankenfehler",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "gedankenfehler",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./src/assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.lafisrap.gedankenfehler"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/favicon.png",
            "publicPath": "/gedankenfehler/"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true,
            "baseUrl": "/gedankenfehler"
        },
        "extra": {
            "router": {
                "basePath": "/gedankenfehler"
            },
            "eas": {
                "projectId": "4665390e-d5da-4913-a96b-c9fc4c15029d"
            }
        }
    }
}
